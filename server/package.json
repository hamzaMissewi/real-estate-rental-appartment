        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.13",
        "@types/node": "^22.13.0",
        "@types/terraformer__wkt": "^2.0.3",
        "@types/uuid": "^10.0.0",
        "concurrently": "^9.1.2",
        "nodemon": "^3.1.9",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.8.3"
    }
